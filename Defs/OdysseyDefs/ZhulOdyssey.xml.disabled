<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Zhul Odyssey DLC Integration -->
  
  <!-- Zhul Ship Faction -->
  <FactionDef ParentName="FactionBase">
    <defName>ZHUL_SpaceTribe</defName>
    <label>Zhul space tribe</label>
    <description>A nomadic fleet of Zhul cannibals who have taken to the stars, spreading their moustache worship and cannibalistic practices across the galaxy. They travel in bio-ships grown from massive space-born organisms.</description>
    <pawnSingular>Zhul void-walker</pawnSingular>
    <pawnsPlural>Zhul void-walkers</pawnsPlural>
    <categoryTag>Tribal</categoryTag>
    <listOrderPriority>40</listOrderPriority>
    <settlementGenerationWeight>0.3</settlementGenerationWeight>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <maxCountAtGameStart>3</maxCountAtGameStart>
    <canMakeRandomly>true</canMakeRandomly>
    <canSiege>false</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>void chief</leaderTitle>
    <factionIconPath>UI/Icons/Factions/ZhulSpaceTribe</factionIconPath>
    <colorSpectrum>
      <li>(0.45, 0.47, 0.35)</li>
      <li>(0.55, 0.45, 0.39)</li>
      <li>(0.53, 0.55, 0.43)</li>
    </colorSpectrum>
    <requiredMemes>
      <li>Cannibal</li>
      <li>ZHUL_MoustacheWorship</li>
    </requiredMemes>
    <structureMemeWeights>
      <Ideological>1</Ideological>
    </structureMemeWeights>
    <backstoryFilters>
      <li>
        <categories>
          <li>Tribal</li>
          <li>Pirate</li>
        </categories>
        <commonality>1</commonality>
      </li>
    </backstoryFilters>
    <geneticVariance>0.8</geneticVariance>
    <xenotypeSet>
      <xenotypeChances>
        <ZHUL_Xenotype>0.9</ZHUL_Xenotype>
        <Baseliner>0.1</Baseliner>
      </xenotypeChances>
    </xenotypeSet>
    <techLevel>Spacer</techLevel>
    <allowedCultures>
      <li>Astropolitan</li>
    </allowedCultures>
    <factionNameMaker>NamerFactionZhulSpace</factionNameMaker>
    <settlementNameMaker>NamerSettlementZhulFleet</settlementNameMaker>
    <allowedArrivalTemperatureRange>-40~45</allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <ZHUL_VoidWalker>10</ZHUL_VoidWalker>
          <ZHUL_SpaceSpiritEater>5</ZHUL_SpaceSpiritEater>
          <ZHUL_VoidChief>2</ZHUL_VoidChief>
        </options>
      </li>
    </pawnGroupMakers>
  </FactionDef>

  <!-- Zhul Bio-Ship -->
  <ThingDef ParentName="BaseSpaceship">
    <defName>ZHUL_BioShip</defName>
    <label>Zhul bio-ship</label>
    <description>A living spacecraft grown from enormous space-dwelling organisms. The Zhul have cultivated these creatures for generations, turning them into vessels that pulse with organic life. The ship's hull is covered in elaborate patterns resembling giant moustaches.</description>
    <graphicData>
      <texPath>Things/Building/Ship/ZhulBioShip</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(15, 30)</drawSize>
    </graphicData>
    <size>(15, 30)</size>
    <altitudeLayer>Building</altitudeLayer>
    <passability>Impassable</passability>
    <blockWind>true</blockWind>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>1</fillPercent>
    <blockLight>true</blockLight>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>5000</MaxHitPoints>
      <Flammability>0.2</Flammability>
      <Beauty>10</Beauty>
    </statBases>
    <tickerType>Normal</tickerType>
    <comps>
      <li Class="CompProperties_ShipPart">
        <isShipPart>true</isShipPart>
      </li>
      <li Class="CompProperties_Power">
        <compClass>CompPowerPlant</compClass>
        <basePowerConsumption>-3000</basePowerConsumption>
        <transmitsPower>true</transmitsPower>
      </li>
      <li Class="CompProperties_BioShip">
        <bioShipClass>ZHUL_BioShipComp</bioShipClass>
        <organicGrowthRate>0.1</organicGrowthRate>
        <healingRate>2.0</healingRate>
        <nutritionConsumption>50</nutritionConsumption>
      </li>
    </comps>
    <building>
      <shipPart>true</shipPart>
    </building>
    <constructionSkillPrerequisite>8</constructionSkillPrerequisite>
    <researchPrerequisites>
      <li>ZHUL_BioShipTechnology</li>
    </researchPrerequisites>
    <costList>
      <Steel>500</Steel>
      <Plasteel>200</Plasteel>
      <ComponentSpacer>10</ComponentSpacer>
      <ZHUL_OrganicMatter>100</ZHUL_OrganicMatter>
    </costList>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
  </ThingDef>

  <!-- Space Zhul Pawn Kinds -->
  <PawnKindDef>
    <defName>ZHUL_VoidWalker</defName>
    <label>Zhul void-walker</label>
    <race>ZHUL_AlienHumanoid</race>
    <defaultFactionType>ZHUL_SpaceTribe</defaultFactionType>
    <chemicalAddictionChance>0.1</chemicalAddictionChance>
    <invNutrition>2.55</invNutrition>
    <itemQuality>Normal</itemQuality>
    <backstoryCryptosleepCommonality>0.25</backstoryCryptosleepCommonality>
    <maxGenerationAge>65</maxGenerationAge>
    <combatPower>80</combatPower>
    <gearHealthRange>0.7~3.2</gearHealthRange>
    <apparelTags>
      <li>SpacerMilitary</li>
      <li>ZHUL_SpaceTribal</li>
    </apparelTags>
    <apparelMoney>1000~2500</apparelMoney>
    <weaponTags>
      <li>SpacerGun</li>
      <li>ZHUL_VoidWeapon</li>
    </weaponTags>
    <weaponMoney>300~700</weaponMoney>
    <techHediffsChance>0.15</techHediffsChance>
    <techHediffsMoney>1000~1500</techHediffsMoney>
    <biocodeWeaponChance>0.2</biocodeWeaponChance>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>10~17</initialResistanceRange>
  </PawnKindDef>

  <PawnKindDef>
    <defName>ZHUL_SpaceSpiritEater</defName>
    <label>space spirit-eater</label>
    <race>ZHUL_AlienHumanoid</race>
    <defaultFactionType>ZHUL_SpaceTribe</defaultFactionType>
    <chemicalAddictionChance>0.05</chemicalAddictionChance>
    <invNutrition>2.55</invNutrition>
    <itemQuality>Good</itemQuality>
    <backstoryCryptosleepCommonality>0.25</backstoryCryptosleepCommonality>
    <maxGenerationAge>65</maxGenerationAge>
    <combatPower>120</combatPower>
    <gearHealthRange>0.7~3.2</gearHealthRange>
    <apparelTags>
      <li>SpacerMilitary</li>
      <li>ZHUL_ShamanRobes</li>
    </apparelTags>
    <apparelMoney>1500~3000</apparelMoney>
    <weaponTags>
      <li>SpacerGun</li>
      <li>ZHUL_RitualWeapon</li>
    </weaponTags>
    <weaponMoney>500~1000</weaponMoney>
    <techHediffsChance>0.25</techHediffsChance>
    <techHediffsMoney>1500~2500</techHediffsMoney>
    <biocodeWeaponChance>0.3</biocodeWeaponChance>
    <initialWillRange>3~5</initialWillRange>
    <initialResistanceRange>15~25</initialResistanceRange>
    <abilities>
      <li>ZHUL_SpiritChanneling</li>
      <li>ZHUL_MoustacheMeditation</li>
    </abilities>
  </PawnKindDef>

  <!-- Zhul Space Quest -->
  <QuestScriptDef>
    <defName>ZHUL_VoidCannibalEncounter</defName>
    <label>void cannibal encounter</label>
    <description>A Zhul bio-ship has been detected approaching your location. These space-faring cannibals seek fresh meat for their void rituals.</description>
    <questNameRules>
      <rulesStrings>
        <li>questName->Void Cannibal [encounter]</li>
        <li>encounter->Approach</li>
        <li>encounter->Ritual</li>
        <li>encounter->Hunt</li>
      </rulesStrings>
    </questNameRules>
    <questDescriptionRules>
      <rulesStrings>
        <li>questDescription->The [zhulship] approaches seeking [goal].</li>
        <li>zhulship->bio-ship</li>
        <li>zhulship->void vessel</li>
        <li>zhulship->living craft</li>
        <li>goal->fresh meat for rituals</li>
        <li>goal->victims for the void feast</li>
        <li>goal->sacrifices to the moustache spirits</li>
      </rulesStrings>
    </questDescriptionRules>
    <root Class="QuestNode_Sequence">
      <nodes>
        <li Class="QuestNode_SubScript">
          <def>Util_GenerateFaction</def>
          <parms>
            <storeAs>faction</storeAs>
            <factionDef>ZHUL_SpaceTribe</factionDef>
          </parms>
        </li>
        <li Class="QuestNode_SpawnSkyfaller">
          <skyfallerDef>ZHUL_BioShipIncoming</skyfallerDef>
          <factionOf>$faction</factionOf>
          <spawnNear>[map_playerHome]</spawnNear>
        </li>
        <li Class="QuestNode_Letter">
          <label>Void Cannibals Approach</label>
          <text>A massive bio-ship has appeared in orbit. Sensors detect Zhul life signs aboard - space-faring cannibals known for their ritualistic consumption of sentient beings. They seem to be preparing for some kind of ceremony.</text>
          <letterDef>ThreatBig</letterDef>
        </li>
      </nodes>
    </root>
  </QuestScriptDef>

  <!-- Organic Ship Materials -->
  <ThingDef ParentName="ResourceBase">
    <defName>ZHUL_OrganicMatter</defName>
    <label>organic ship matter</label>
    <description>Living tissue harvested from space-dwelling organisms. The Zhul use this material to grow and repair their bio-ships. It pulses with an eerie life force.</description>
    <graphicData>
      <texPath>Things/Item/Resource/ZhulOrganicMatter</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
    </graphicData>
    <soundInteract>Metal_Drop</soundInteract>
    <soundDrop>Metal_Drop</soundDrop>
    <stackLimit>75</stackLimit>
    <healthAffectsPrice>false</healthAffectsPrice>
    <statBases>
      <MaxHitPoints>70</MaxHitPoints>
      <MarketValue>8.5</MarketValue>
      <Mass>0.8</Mass>
      <Flammability>0.4</Flammability>
      <DeteriorationRate>0.5</DeteriorationRate>
    </statBases>
    <intricate>false</intricate>
    <thingCategories>
      <li>ResourcesRaw</li>
    </thingCategories>
    <burnableByRecipe>true</burnableByRecipe>
    <smeltable>false</smeltable>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>10</daysToRotStart>
        <daysToDessicated>25</daysToDessicated>
        <rotDestroys>true</rotDestroys>
      </li>
    </comps>
  </ThingDef>

  <!-- Bio-Ship Technology Research -->
  <ResearchProjectDef>
    <defName>ZHUL_BioShipTechnology</defName>
    <label>bio-ship cultivation</label>
    <description>Learn to cultivate and control living spacecraft using Zhul organic manipulation techniques. Requires studying captured organic matter from their vessels.</description>
    <baseCost>4000</baseCost>
    <techLevel>Spacer</techLevel>
    <prerequisites>
      <li>ShipBasics</li>
      <li>Xenogenetics</li>
    </prerequisites>
    <researchViewX>14.00</researchViewX>
    <researchViewY>5.90</researchViewY>
    <requiredResearchBuilding>HiTechResearchBench</requiredResearchBuilding>
    <requiredResearchFacilities>
      <li>MultiAnalyzer</li>
    </requiredResearchFacilities>
    <requiredAnalyzedThings>
      <li>ZHUL_OrganicMatter</li>
    </requiredAnalyzedThings>
  </ResearchProjectDef>

</Defs>