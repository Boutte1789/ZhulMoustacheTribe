  <!-- Curled Savage - Basic Zhul Warrior -->
  <PawnKindDef>
    <defName>ZHUL_CurledSavage_PrepareCarefully</defName>
    <label>Curled Savage</label>
    <race>ZHUL_AlienHumanoid</race>
    <defaultFactionType>ZhulTribe</defaultFactionType>
    <chemicalAddictionChance>0.1</chemicalAddictionChance>
    <invNutrition>2.5</invNutrition>
    <itemQuality>Poor</itemQuality>
    <backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
    <minGenerationAge>16</minGenerationAge>
    <maxGenerationAge>45</maxGenerationAge>
    <combatPower>80</combatPower>
    <gearHealthRange>0.7~1.0</gearHealthRange>
    <apparelMoney>200~400</apparelMoney>
    <apparelAllowHeadgearChance>0.3</apparelAllowHeadgearChance>
    <weaponMoney>200~500</weaponMoney>
    <techHediffsMoney>50~150</techHediffsMoney>
    <techHediffsChance>0.1</techHediffsChance>
    <initialWillRange>1~3</initialWillRange>
    <initialResistanceRange>8~14</initialResistanceRange>
    <canArriveManhunter>false</canArriveManhunter>
    <forceNormalGearQuality>true</forceNormalGearQuality>
    
    <backstoryCategories>
      <li>ZhulTribal</li>
      <li>Tribal</li>
    </backstoryCategories>
    
    <skills>
      <li>
        <skill>Melee</skill>
        <range>6~12</range>
      </li>
      <li>
        <skill>Shooting</skill>
        <range>4~8</range>
      </li>
      <li>
        <skill>Social</skill>
        <range>3~7</range>
      </li>
      <li>
        <skill>Animals</skill>
        <range>2~6</range>
      </li>
    </skills>
    
    <apparelTags>
      <li>Neolithic</li>
      <li>Tribal</li>
    </apparelTags>
    
    <weaponTags>
      <li>NeolithicMelee</li>
      <li>NeolithicRanged</li>
    </weaponTags>
    
    <forcedTraits>
      <!-- Removed invalid trait ZHUL_MoustacheRitual -->
      <li>
        <def>ZHUL_Cannibal</def>
        <chance>0.8</chance>
      </li>
    </forcedTraits>
    
    <bodyTypeList>
      <li>FemaleNarrow</li>
