    <thingClass>Pawn</thingClass>
    <race>
      <body>Human</body>
      <baseBodySize>1.0</baseBodySize>
      <baseHealthScale>1.0</baseHealthScale>
      <baseHungerRate>1.0</baseHungerRate>
      <baseAgeGenerationRange>
        <min>15</min>
        <max>80</max>
      </baseAgeGenerationRange>
      <foodType>OmnivoreHuman</foodType>
      <wildBiomes>
        <li>
          <biome>TropicalRainforest</biome>
          <weight>1.0</weight>
        </li>
      </wildBiomes>
    </race>
    <alienRace>
      <generalSettings>
        <alienPartGenerator>
          <bodyAddons />
        </alienPartGenerator>
        <graphicPaths>
          <li>Things/Pawn/Humanlike/Bodies/Zhul</li>
        </graphicPaths>
        <primaryRace>ZHUL_Race</primaryRace>
      </generalSettings>
      <bodySettings>
        <headTypes>
          <li>Normal</li>
          <li>Narrow</li>
        </headTypes>
      </bodySettings>
    </alienRace>
  </ThingDef>
</Defs>